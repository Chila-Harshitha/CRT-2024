#include <stdio.h>

int main() {
    // Write C code here
    int a,b,choice;
    scanf("%d%d",&a,&b);
    printf("Enter choice");
    scanf("%d",&choice);
    switch(choice)
    {
        case 1:printf("%d",a+b);
        break;
        case 2:printf("%d",a-b);
        break;
        case 3:printf("%d",a*b);
        break;
        case 4:printf("%d",a/b);
        break;
        default:printf("Invalid");
        break;
    }
    
    
    return 0;
}
